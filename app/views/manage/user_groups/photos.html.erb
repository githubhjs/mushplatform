<div class="right2">
  <%= render :partial => "/manage/user_groups/per_group_menu" %>
  <div class="height20"></div>
  <div class="w90">
    <div class="title1">
      <p><img src="/images/new/xjt.jpg" align="absmiddle"> 群组相册</p>
    </div>
    <div class="height20"></div>
    <div class="photo2b">
      <div class="height20"></div>
      <ul>
        <% for pic in @photos %>
          <li><a href="/manage/user_groups/<%= @user_group.id %>/photo?photo_id=<%= pic.id %>"><img  src="<%= pic.mid_link %>"></a>
            <p><%= pic.title %></p>
            <a href="/manage/user_groups/<%= @user_group.id %>/edit_photo?photo_id=<%= pic.id %>">编辑</a>
            <%= link_to "删除","/manage/user_groups/#{@user_group.id}/delete_photo?photo_id=#{pic.id}",:confirm => "确认要删除吗" %>
            <br>
          </li>
        <%end%>
      </ul>
    </div>
    <div class="height20"></div>
  </div>
  <div class="clear"></div>
</div>

