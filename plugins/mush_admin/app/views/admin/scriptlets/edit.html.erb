<script>set_current_channel('cms')</script>
<h2>Manage Scriptlets (<%= link_to 'add new', :controller => 'scriptlets', :action => 'new' %>)</h2>


<% form_tag('/admin/scriptlets/update', :multipart => true ) do %>

  <ul class="subsubsub">
    <li><a class="current"><%= @scriptlet.name %></a></li> |
    <li><a class="current"><%= link_to 'Cancel', scriptlets_path, :class => 'cancel' %></a></li>
  </ul>

  <%= render :partial => 'form' %>
  
  <div id="ajax-response"></div>

  <div class="tablenav">
      <br class="clear" />
  </div>

  <br class="clear" />
  
<% end -%>
