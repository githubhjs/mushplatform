<div class="right2">
  <%= render :partial => "/manage/friends/friends_menu" %>
  <div class="w90">
    <div class="height20"></div>
    <div class="friend">
      <ul>
        <% for visitor in @visitores %>
          <li><p><a href="<%= visitor.space_url %>" target="_blank"><img src="<%= user_icon(visitor.visitor_id) %>"></a></p>
            <p style="overflow: hidden; width: 78px; height: 22px;"><a href="<%= visitor.space_url %>" target="_blank"><%= visitor.visitor_real_name %></a></p
            <span><%= visitor.created_at.strftime('%y-%m-%d %H:%M') %></span>
          </li>
        <%end%>
      </ul>
    </div>
    <div class="height20"></div>
    <div class="jogger">
      <%=  %>
    </div>
  </div>
</div>