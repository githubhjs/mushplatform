<script>set_current_channel('cms')</script>
<h2>Manage Assets (<%= link_to 'add new', :controller => 'assets', :action => 'new' %>)</h2>


<% form_tag('/admin/assets/update', :multipart => true ) do %>

  <ul class="subsubsub">
    <li><a class="current"><%= @asset.filename %></a></li> |
    <li><%= link_to 'Cancel', {:action => 'index', :path => @asset.parent_path},  :class => 'cancel' %></li>
  </ul>

  <%= render :partial => 'form' %>
  
  <div id="ajax-response"></div>

  <div class="tablenav">
      <br class="clear" />
  </div>

  <br class="clear" />
  
<% end -%>
