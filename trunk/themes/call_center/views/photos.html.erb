<div class="main">
  <div class="left">
    <%= render :partial => "/share/myinfo" %>
    <div class="height12"></div>
    <%= render :partial => "/share/music" %>
    <div class="height12"></div>
  </div>
  <div class="right2">
    <div class="box2">
      <div class="title2"><p>我的相册</p>
        <% if is_blog_admin? %>
          <span><img src="/images/call_center/tu_sczp.gif"> <a href="/manage/photos/new"><strong>上传照片</strong></a></span>
        <%end%>
      </div>
      <div class="tabs6">
        <ul>
          <li class="tabs66"><a href="/photos" title="全部照片"><span>全部照片</span></a></li>
        </ul>
      </div>
      <div class="height20"></div>
      <div class="height20"></div>
      <div class="photo">
        <ul>
          <% for photo in @photos %>
            <li><a href="/photos/<%= photo.id %>"><img src="<%= photo.mid_link %>" ></a>
              <p><a href="/photos/<%= photo.id %>"><%= photo.title %></a></p>
            </li>
          <%end%>
        </ul>
      </div>
      <div class="height20"></div>
      <div class="scott">
        <%= will_paginate @photos,{:page_url => '/photos', :prev_label => "<<上一页",:next_label => '下一页>>'} %>
      </div>
      <div class="height20"></div>
      <div class="height20"></div>
    </div>
  </div>
</div>